<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <script src="/js/jquery-3.1.1.min.js"></script>
</head>
<body>
<div>
    <h2>首页</h2>
    <p>
        用户名：<input type="text" id="name"/>
    </p>
    <p>
        密码：<input type="text" id="password"/>
    </p>
    <input type="submit" id="sub" value="提交"/>
</div>
</body>
</html>
<script>
    $(function(){
        $('#sub').on('click',function(){
            $.post('/dopost',{
                name:$('#name').val(),
                password:$('#password').val()
            },function(result){
                console.log(result);
            })
        })
    })
</script>